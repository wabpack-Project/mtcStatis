.list-page{
	text-align: right;
}
.list-page button{
	margin: 0;
	border: none;
}
.list-page button,.list-page .button-ellipsis{
	padding: 0px 9px;
	color: #337ab7;
	height: 30px;
	line-height: 30px;
	background: #ddd;
}
.list-page button:first-child{
	border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
}
.list-page button:last-of-type{
	border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
}
.list-page button:hover{
	color: #23527C;
}
.list-page button:disabled{
	color: #777;
	cursor: not-allowed;
}
.list-page button.current,.list-page button.current:hover{
	color: #fff;
	cursor: default;
}
.list-page .button-ellipsis{
	display: inline-block;
	cursor: default;
}
.noData{
	display: none;
	text-align: center;
}